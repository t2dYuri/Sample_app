<% provide(:title, "Sign in") %>
<% breadcrumb :enter %>
<h1>Sign in</h1>
<div class="hero-unit">
  <div class="row">
    <div class="col-md-8 col-md-offset-4">
    <%= form_for(:session, url: sessions_path) do |f| %>
      <%= f.label :email %><br />
      <%= f.text_field :email, class:"input1" %><br />
      <%= f.label :password %><br />
      <%= f.password_field :password, class:"input1" %><br />
      <%= f.submit "Sign in", class: "btn btn-primary" %>
    <% end %>
    </div>
  </div>
</div>
<div class="alert alert-info" role="alert" align="center">Еще не зарегистрирован? <%= link_to "Зарегайся!", signup_path %></div>